name: mlflow_rf

conda: conda.yaml
entry_points:
  main:
    parameters:     
       n_estimators: {type: int, default: 100}
       min_samples_split: {type: int, default: 2}
       min_samples_leaf: {type: int, default: 2}
    command: "python train_rf_classifier.py {n_estimators} {min_samples_split} {min_samples_leaf}"
